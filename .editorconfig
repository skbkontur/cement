# http://editorconfig.org
root = true

[*]
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true
indent_style = space
indent_size = 4
end_of_line = crlf

[*.md]
trim_trailing_whitespace = false

[*.{config,csproj}]
indent_size = 2
insert_final_newline = false
trim_trailing_whitespace = false

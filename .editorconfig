# editorconfig.org

root = true

[*]
insert_final_newline = true
charset = utf-8
end_of_line = crlf
trim_trailing_whitespace = true
indent_style = space
indent_size = 4

[*.md]
trim_trailing_whitespace = false

[*.{config,csproj}]
insert_final_newline = false
trim_trailing_whitespace = false
indent_size = 2
